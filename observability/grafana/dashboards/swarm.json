{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Proposals (approved / rejected / pending)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "custom": { "lineWidth": 2, "fillOpacity": 15 } },
        "overrides": []
      },
      "targets": [
        {
          "expr": "rate(swarm_proposal_count_total[1m])",
          "legendFormat": "{{result}} ({{type}})",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Policy Violations",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "red", "value": 1 }] } },
        "overrides": []
      },
      "targets": [
        {
          "expr": "swarm_policy_violation_count_total",
          "legendFormat": "violations",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Agent Errors",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "orange", "value": 1 }, { "color": "red", "value": 5 }] } },
        "overrides": []
      },
      "targets": [
        {
          "expr": "swarm_agent_error_count_total",
          "legendFormat": "{{role}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Governance Loop Latency (ms)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "unit": "ms", "color": { "mode": "thresholds" }, "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 1000 }, { "color": "red", "value": 5000 }] } },
        "overrides": []
      },
      "targets": [
        {
          "expr": "rate(swarm_governance_loop_ms_sum[5m]) / rate(swarm_governance_loop_ms_count[5m])",
          "legendFormat": "avg",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Agent Latency by Role (ms)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "unit": "ms", "color": { "mode": "palette-classic" }, "custom": { "lineWidth": 2, "fillOpacity": 10 } },
        "overrides": []
      },
      "targets": [
        {
          "expr": "rate(swarm_agent_latency_ms_sum[5m]) / rate(swarm_agent_latency_ms_count[5m])",
          "legendFormat": "{{role}} avg",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Agent Latency p95 by Role",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "unit": "ms", "color": { "mode": "palette-classic" }, "custom": { "lineWidth": 2, "fillOpacity": 10 } },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(swarm_agent_latency_ms_bucket[5m]))",
          "legendFormat": "p95 {{role}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Proposal Throughput (per minute)",
      "type": "bargauge",
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "thresholds" }, "thresholds": { "steps": [{ "color": "blue", "value": null }] } },
        "overrides": []
      },
      "targets": [
        {
          "expr": "increase(swarm_proposal_count_total[1m])",
          "legendFormat": "{{result}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Agent Invocations (total)",
      "type": "bargauge",
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" } },
        "overrides": []
      },
      "targets": [
        {
          "expr": "swarm_agent_latency_ms_count",
          "legendFormat": "{{role}}",
          "refId": "A"
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "tags": ["swarm", "agents"],
  "templating": {
    "list": [
      {
        "current": { "selected": false, "text": "Prometheus", "value": "Prometheus" },
        "hide": 0,
        "includeAll": false,
        "name": "DS_PROMETHEUS",
        "query": "prometheus",
        "type": "datasource"
      }
    ]
  },
  "time": { "from": "now-30m", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Swarm Governance",
  "uid": "swarm-governance",
  "version": 1
}
